@article {curraneverett13,
	author = {Curran-Everett, Douglas},
	title = {Explorations in statistics: the analysis of ratios and normalized data},
	volume = {37},
	number = {3},
	pages = {213--219},
	year = {2013},
	doi = {10.1152/advan.00053.2013},
	publisher = {American Physiological Society},
	abstract = {Learning about statistics is a lot like learning about science: the learning is more meaningful if you can actively explore. This ninth installment of Explorations in Statistics explores the analysis of ratios and normalized{\textendash}or standardized{\textendash}data. As researchers, we compute a ratio{\textendash}a numerator divided by a denominator{\textendash}to compute a proportion for some biological response or to derive some standardized variable. In each situation, we want to control for differences in the denominator when the thing we really care about is the numerator. But there is peril lurking in a ratio: only if the relationship between numerator and denominator is a straight line through the origin will the ratio be meaningful. If not, the ratio will misrepresent the true relationship between numerator and denominator. In contrast, regression techniques{\textendash}these include analysis of covariance{\textendash}are versatile: they can accommodate an analysis of the relationship between numerator and denominator when a ratio is useless.},
	issn = {1043-4046},
	URL = {http://advan.physiology.org/content/37/3/213},
	eprint = {http://advan.physiology.org/content/37/3/213.full.pdf},
	journal = {Advances in Physiology Education}
}

